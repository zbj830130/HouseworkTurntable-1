<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.bignerdranch.android.houseworkturntable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".OptionsList"
            android:label="@string/title_activity_options_list"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".HouseworkItemActivity">
        </activity>
        <activity
            android:name=".HouseworkItemViewPageActivity"
            android:label="@string/title_activity_housework_item_view_page"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".ChartActivity"
            android:label="@string/title_activity_chart"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".HouseworkPanActivity">
        </activity>
    </application>

</manifest>